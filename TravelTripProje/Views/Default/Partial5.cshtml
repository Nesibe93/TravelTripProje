    {
        <div class="bot-img">
            <img src="@item.BlogImage" alt="" style="height:70px; width:120px;"/ />
        </div>
        <div class="bot-desc1">
            <p>@item.Aciklama.Substring(0,80)...</p>
        </div>
    <div class="clear"></div>
    }
</div>
